{
  "compilerOptions": {
    "jsx": "react",
    "module": "ESNext",
    "noImplicitAny": true,
    "outDir": "./build/",
    "preserveConstEnums": true,
    "sourceMap": true,
    "target": "es5",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node"
  },
  "include": [
    "src/**/*"
  ]
}